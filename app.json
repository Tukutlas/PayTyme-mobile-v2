{
  "expo": {
    "name": "PayTyme",
    "slug": "PayTyme",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.victorel.paytymemobileapp",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to take pictures.",
        "NSDocumentPickerUsageDescription": "This app requires access to documents for file selection.",
        "NSTouchIDUsageDescription": "We use Touch ID to secure your data."
      },
      "buildNumber": "5"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "blockedPermissions": [
        "RECORD_AUDIO", 
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION"
      ],
      "permissions": [
        "CAMERA",
        "CAMERA_ROLL",
        "USE_BIOMETRIC",
        "USE_FINGERPRINT",
        "READ_CONTACTS",
        "READ_EXTERNAL_STORAGE"
      ],
      "package": "com.victorel.paytymemobileapp",
      "versionCode": 8
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "024cfd22-e17c-45ce-b7dc-f648f7bc098d"
      }
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow PayTyme to access your camera."
        }
      ]
    ]
  }
}
